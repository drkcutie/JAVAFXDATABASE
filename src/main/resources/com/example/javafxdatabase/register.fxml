<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="263.0" prefWidth="228.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdatabase.RegisterController">
   <children>
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#backtoLogin" text="Back" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Register Account">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
      <TextField fx:id="registerUsername" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <TextField fx:id="registerPassword" />
      <Button fx:id="accountRegister" mnemonicParsing="false" onAction="#accountRegister" text="Register" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
